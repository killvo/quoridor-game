{
  "$schema": "https://json.schemastore.org/swcrc",
  "minify": false,
  "sourceMaps": true,
  "module": {
    "type": "es6",
    "strict": true,
    "noInterop": true
  },
  "jsc": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    },
    "target": "ESNext",
    "keepClassNames": true,
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "decoratorMetadata": true
    }
  }
}
